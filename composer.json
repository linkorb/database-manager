{
    "name": "linkorb/database-manager",
    "description": "Database Management utilities: connectivity, backups, fixtures, etc",
    "homepage": "http://www.github.com/linkorb/database-manager",
    "keywords": ["php", "api", "database", "fixtures", "backup", "mysql", "postgresql", "pdo", "doctrine"],
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Joost Faassen",
        "email": "j.faassen@linkorb.com",
        "role": "Development"
    }],
    "require-dev": {
        "symfony/console": "~3.0",
        "phpunit/phpunit": "~4.6",
        "linkorb/conventions-checker": "~1.1"
    },
    "extra": {
        "grumphp": {
            "config-default-path": "vendor/linkorb/conventions-checker/config/lib/grumphp.yml"
        }
    },
    "autoload": {
        "psr-4": {
            "LinkORB\\Component\\DatabaseManager\\": "src/DatabaseManager"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LinkORB\\Component\\DatabaseManager\\Tests\\": "tests/DatabaseManager"
        }
    },
    "bin": ["bin/database-manager"]
}
